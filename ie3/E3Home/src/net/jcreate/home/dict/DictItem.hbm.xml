<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">	
<hibernate-mapping>
	<class name="net.jcreate.home.dict.DictItem" table="st_dict_item">
		<id name = "oid"
            column="oid"
            unsaved-value = "null">
               <generator class="assigned" />
		</id>
 		<property name="dictOID" />		
 		<property name="dictItemCode"/>
 		<property name="dictItemName"/>
 		<property name="viewOrder"/>
 		<property name="createUserId"/> 		
 		<property name="createDatetime"/> 		
 		<property name="modifyUserId"/> 		 		
 		<property name="modifyDatetime"/> 		 		 		
 		<property name="remark"/>
 	</class>
</hibernate-mapping>